def search(arr, num, factor):

    for i in range(0, num):
        if arr[i] == factor:
            return i
    return -1


array = [2, 4, 0, 1, 9]
x = 1
n = len(array)
result = search(array, n, x)
if result == -1:

    print("Element not found")
else:
    print("Element found at index: ", result)
